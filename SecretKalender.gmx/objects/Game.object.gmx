        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw secret
if (secret) {
    var w = 512;
    var th = 64;
    var lh = 32;
    var x1 = (room_width / 2) - (w / 2);
    var y1 = 64 * 2;
    var lines = 10;
    
    //Draw title frame
    draw_set_colour(c_maroon);
    draw_rectangle(x1, y1, x1 + w, y1 + th, false);
    
    //Draw title text
    draw_set_font(fnt_Arial_Medium);
    draw_set_colour(c_white);
    draw_set_halign(fa_center);
    draw_set_valign(fa_middle);
    draw_text(x1 + (w / 2), y1 + (th / 2), "Deltakere - Luke " + string(secretDay));
    draw_set_halign(fa_left);
    draw_set_valign(fa_top);
    
    //Button settings
    var bx1 = x1;
    var bx2 = x1 + w - th;
    var bc1 = c_green;
    var bc2 = c_green;
    
    //Hover buttons
    if (mouse_y &gt;= y1 &amp;&amp; mouse_y &lt;= y1 + th) {
        if (mouse_x &gt;= bx1 &amp;&amp; mouse_x &lt;= bx1 + th) {
            bc1 = c_lime;
            if (mouse_check_button_pressed(mb_left)) {
                email_load_day(secretDay - 1);
            }
        }
        else if (mouse_x &gt;= bx2 &amp;&amp; mouse_x &lt;= bx2 + th) {
            bc2 = c_lime;
            if (mouse_check_button_pressed(mb_left)) {
                email_load_day(secretDay + 1);
            }
        }
    }
    
    //Draw button frames
    draw_set_colour(bc1);
    draw_rectangle(bx1, y1, bx1 + th, y1 + th, false);
    draw_set_colour(bc2);
    draw_rectangle(bx2, y1, bx2 + th, y1 + th, false);
    
    //Draw button text
    draw_set_font(fnt_Arial_Medium);
    draw_set_colour(c_white);
    draw_set_halign(fa_center);
    draw_set_valign(fa_middle);
    draw_text(bx1 + (th / 2), y1 + (th / 2), "&lt;&lt;");
    draw_text(bx2 + (th / 2), y1 + (th / 2), "&gt;&gt;");
    draw_set_halign(fa_left);
    draw_set_valign(fa_top);
    
    //Draw emails
    var ye = 0;
    for (var i = 0; i &lt; lines; i++) {
        var col = c_black;
        if (i mod 2 == 1) { col = c_dkgray; }
        
        //Draw frame
        ye = (y1 + th) + (lh * i);
        draw_set_colour(col);
        draw_rectangle(x1, ye, x1 + w, ye + lh, false);
        
        if (secretId + i &lt; ds_list_size(secretList)) {
            //Draw email text
            draw_set_font(fnt_Arial_Small);
            draw_set_colour(c_white);
            draw_set_valign(fa_middle);
            draw_text(x1 + 8, ye + (lh / 2), string(secretId + i + 1) + ": " + secretList[| secretId + i]);
            draw_set_valign(fa_top);
        }
    }
    
    //Draw winner frame
    var wy = ye + lh;
    var by2 = wy + lh;
    draw_set_colour(c_maroon);
    draw_rectangle(x1, wy, x1 + w, by2, false);
    
    //Draw winner text
    draw_set_font(fnt_Arial_Small);
    draw_set_colour(c_white);
    draw_set_halign(fa_center);
    draw_set_valign(fa_middle);
    draw_text(x1 + (w / 2), wy + (lh / 2), "Vinner: " + secretWinner);
    draw_set_halign(fa_left);
    draw_set_valign(fa_top);
    
    //Draw button frame
    draw_set_colour(c_maroon);
    draw_rectangle(x1, by2, x1 + w, by2 + th, false);
    
    var ic1 = c_blue;
    var ic2 = c_blue;
    if (mouse_y &gt;= by2 &amp;&amp; mouse_y &lt;= by2 + th) {
        if (mouse_x &gt;= bx1 &amp;&amp; mouse_x &lt;= bx1 + th) {
            ic1 = c_aqua;
            if (mouse_check_button_pressed(mb_left)) {
                if (secretId - lines &gt;= 0) { secretId -= lines; }
            }
        }
        else if (mouse_x &gt;= bx2 &amp;&amp; mouse_x &lt;= bx2 + th) {
            ic2 = c_aqua;
            if (mouse_check_button_pressed(mb_left)) {
                if (secretId + lines &lt; ds_list_size(secretList)) { secretId += lines; }
            }
        }
    }
    
    //Draw button frames
    draw_set_colour(ic1);
    draw_rectangle(bx1, by2, bx1 + th, by2 + th, false);
    draw_set_colour(ic2);
    draw_rectangle(bx2, by2, bx2 + th, by2 + th, false);
    
    //Draw button text
    draw_set_font(fnt_Arial_Medium);
    draw_set_colour(c_white);
    draw_set_halign(fa_center);
    draw_set_valign(fa_middle);
    draw_text(bx1 + (th / 2), by2 + (th / 2), "/\");
    draw_text(bx2 + (th / 2), by2 + (th / 2), "\/");
    draw_set_halign(fa_left);
    draw_set_valign(fa_top);
    
    var bw = (w - (th * 2)) / 2;
    var bcx = x1 + th;
    var bwx = x1 + w - th - bw;
    
    var cc = c_purple;
    var wc = c_green;
    if (mouse_y &gt;= by2 &amp;&amp; mouse_y &lt;= by2 + th) {
        if (mouse_x &gt;= bcx &amp;&amp; mouse_x &lt;= bcx + bw) {
            cc = c_fuchsia;
            if (mouse_check_button_pressed(mb_left)) {
                var q = "Er du sikker på at du vil slette alle deltakere for Luke " + string(secretDay) + "?";
                if (show_question(q)) {
                    email_clear(secretDay);
                    email_load_day(secretDay);
                }
            }
        }
        else if (mouse_x &gt;= bwx &amp;&amp; mouse_x &lt;= bwx + bw) {
            wc = c_lime;
            if (mouse_check_button_pressed(mb_left)) {
                if (secretWinner != "&lt;Ikke valgt&gt;") {
                    email_select_winner(secretDay);
                    email_load_day(secretDay);
                }
                else {
                    var q = "Er du sikker på at du vil trekke en vinner for Luke " + string(secretDay) + "?";
                    if (show_question(q)) {
                        email_select_winner(secretDay);
                        email_load_day(secretDay);
                    }
                }
            }
        }
    }
    
    //Draw button frames
    draw_set_colour(cc);
    draw_rectangle(bcx, by2, bcx + bw, by2 + th, false);
    draw_set_colour(wc);
    draw_rectangle(bwx, by2, bwx + bw, by2 + th, false);
    
    //Draw button text
    draw_set_font(fnt_Arial_Small);
    draw_set_colour(c_white);
    draw_set_halign(fa_center);
    draw_set_valign(fa_middle);
    draw_text(bcx + (bw / 2), by2 + (th / 2), "Reset luke");
    draw_text(bwx + (bw / 2), by2 + (th / 2), "Trekk vinner");
    draw_set_halign(fa_left);
    draw_set_valign(fa_top);
}
</string>
          </argument>
        </arguments>
      </action>
