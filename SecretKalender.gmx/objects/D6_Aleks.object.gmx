            <kind>1</kind>
            <string>///Events
if (!Dialog.active) {
    if (gameDone) {
        room_goto(rom_Calendar);
    }
    if (!combat) {
        //Check for level enter
        if (keyboard_check_pressed(vk_space) &amp;&amp; currentPointId &gt; 0) {
            //Set background
            with (D6_ActionPoint) { visible = false; }
            background_visible[0] = false;
            background_colour = c_black;
            
            //Move player
            x = 64 * 4;
            y = room_height - (64 * 3);
            
            //Spawn sword
            var sword = instance_create(x, y, D6_Sword);
            combat = true;
            
            //Spawn enemies
            var isBoss = (currentPointId == 5 || currentPointId == 10);
            var spawnNum = random_int(2, 4);
            if (isBoss) { spawnNum = 1; }
            for (var i = 0; i &lt; spawnNum; i++) {
                var randX = random_int(room_width / 2, room_width - 64);
                var randY = y;
                var enemy = instance_create(randX, randY, D6_Enemy);
                var realLvl = currentPointId + ((currentPointId - 1) * 2);
                enemy.level = realLvl;
                enemy.isBoss = isBoss;
                enemy.image_index = currentPointId - 1;
            }
            
            //Show dialog
            if (firstCombat) {
                firstCombat = false;
                dialog_show("Combat Intro");
            }
        }
    }
    else {
        if (hold) {
            //Move
            var dragWeight = 16;
            x += (mouse_x - x) / dragWeight;
            y += (mouse_y - y) / dragWeight;
        }
        else {
            //Gravity
            y += 12;
            var groundY = room_height - (64 * 2.5);
            if (y &gt; groundY) { y = groundY; }
        }
    }
}
</string>
