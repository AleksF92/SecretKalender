            <kind>1</kind>
            <string>///Events
if (!Dialog.active) {
    if (gameDone) {
        room = rom_Calendar;
    }
    else {
        if (saved &gt;= req) {
            gameDone = true;
            dialog_show("Win");
        }
        else if (saved + (friendCount - kills - saved) &lt; req) {
            gameDone = true;
            dialog_show("Fail: Friends");
        }
    }
    
    //Movement
    var moveX = keyboard_check(ord("D")) - keyboard_check(ord("A"));
    var moveY = keyboard_check(ord("S")) - keyboard_check(ord("W"));
    var moveLenX = moveSpeed * moveX;
    var moveLenY = moveSpeed * moveY;
    if (collision_line(x, y, x + moveLenX, y, D14_Wall, true, true)) { moveX = 0; }
    if (collision_line(x, y, x, y + moveLenY, D14_Wall, true, true)) { moveY = 0; }
    
    if (moveX != 0) {
        x += moveLenX;
        if (x &lt; wallLeft) { x = wallLeft; }
        else if (x &gt; wallRight) { x = wallRight; }
    }
    if (moveY != 0) {
        y += moveLenY;
        if (y &lt; wallTop) { y = wallTop; }
        else if (y &gt; wallBottom) { y = wallBottom; }
    }
}
</string>
