            <string>///Events
if (!locked &amp;&amp; !Dialog.active) {
    if (mouse_check_button_pressed(mb_left)) {
        if (distance_to_point(mouse_x, mouse_y) &lt;= range) {
            if (!D22_Game.held) {
                D22_Game.held = id;
                D22_Spot.alarm[1] = 1;
            }
        }
    }
    else if (mouse_check_button_released(mb_left)) {
        if (D22_Game.held == id) {
            D22_Game.held = noone;
            var pos = instance_nearest(x, y, D22_Spot);
            if (distance_to_object(pos) &lt;= pos.range) {
                //Drop
                if (pos.good) {
                    //Add to good list
                    ds_list_add(pos.list, id);
                    locked = true;
                }
                else {
                    //Delete word
                    instance_destroy();
                }
            }
        }
    }
}


if (D22_Game.held == id) {
    x = mouse_x;
    y = mouse_y;
}
</string>
