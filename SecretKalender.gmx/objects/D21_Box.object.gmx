          <argument>
            <kind>1</kind>
            <string>///Events

//Fall til grounded
if (ground) {
    if (control &amp;&amp; !Dialog.active) {
        if (!keyboard_check(vk_space)) {
            if (gameDone) {
                room_goto(rom_Calendar);
            }
        
            //Check win
            if (x &gt; room_width) {
                gameDone = true;
                dialog_show("Win");
            }
        
            //Reset
            if (reset) {
                x = startX;
                reset = false;
                D21_Kristian.alarm[1] = room_speed * 0.5;
            }
        
            //Check for kristian
            var dist = D21_Kristian.x - x;
            dist = max(dist, -dist);
            if (dist &lt; 32 &amp;&amp; !D21_Kristian.alarm[1]) {
                dialog_show("Fail");
                reset = true;
            }
        
            if (moving) {
                x += 1;
            }
            else {
                if (!alarm[0]) {
                    alarm[0] = room_speed * 1;
                }
                else {
                    y -= 1;
                }
            }
        }
        else {
            if (!(y &gt;= groundLevel)) {
                //Abort
                alarm[0] = 0;
                moving = false;
                ground = false;
            }
        }
    }
}
else {
    //Fall
    fallSpeed++;
    if (fallSpeed &gt; 20) { fallSpeed = 20; }
    y += fallSpeed;
    
    if (y &gt;= groundLevel) {
        ground = true;
        y = groundLevel;
        if (D21_Kristian.step &lt; 7) { D21_Kristian.step++; }
    }
    else if (y &gt;= groundLevel - 64) {
        D21_Stig.visible = false;
    }
}

if (fadeIn) {
    image_alpha += 0.02;
    if (image_alpha &gt;= 1) {
        image_alpha = 1;
        fadeIn = false;
        D21_Stig.step++;
        startX = x;
    }
}
</string>
